#include <iostream>
using namespace std;
 
int main() {
   float m1, v1, m2, v2, p1, p2;
 
   cout << "Введите массу и объем первого тела: ";
   cin >> m1 >> v1;
 
   cout << "Введите массу и объем второго тела: ";
   cin >> m2 >> v2;
 
   p1 = m1 / v1;
   p2 = m2 / v2;
 
   if (p1 > p2)
       cout << "Материал первого тела плотнее.";
   else if (p2 > p1)
       cout << "Материал второго тела плотнее.";
   else
       cout << "Плотности одинаковые.";
 
   return 0;
}
 
